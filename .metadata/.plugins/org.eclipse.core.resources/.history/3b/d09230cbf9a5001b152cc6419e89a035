
public class _6__SomeUsefulMathClassMethod {

	public static void main(String[] args) {
		double x = 3.14;
		double y = -10;

		System.out.println("- x: " + x + " | y: " + y + " | max: " + Math.max(x, y) + "_");
		System.out.println("- x: " + x + " | y: " + y + " | min: " + Math.min(x, y) + "_");
	}

}
